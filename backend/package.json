{
  "name": "polystrike-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --transpile-only src/scrapers/index.ts",
    "api": "node --loader ts-node/esm src/api/server.ts",
    "oracle": "ts-node-dev --transpile-only src/oracle/service.ts",
    "unbox:keeper": "ts-node-dev --transpile-only src/oracle/unboxService.ts",
    "price:keeper": "ts-node-dev --transpile-only src/oracle/priceKeeper.ts",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --watch --runInBand",
    "build": "tsc -p tsconfig.json",
    "scrape": "ts-node src/scrapers/index.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed",
    "demo:feeder": "node scripts/demo_price_feeder.js"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.4.5",
    "ethers": "^6.15.0",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.12",
    "@types/supertest": "^2.0.16",
    "@types/ws": "^8.5.13",
    "jest": "^29.7.0",
    "prisma": "^5.22.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
